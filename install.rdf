<?xml version="1.0" encoding="UTF-8"?>
<RDF xmlns="http://www.w3.org/1999/02/22-rdf-syntax-ns#" xmlns:em="http://www.mozilla.org/2004/em-rdf#">
	<Description about="urn:mozilla:install-manifest">
		<em:id>adic@youarefunny.addons.mozilla.org</em:id>
		<em:type>2</em:type>
		<em:name>ADIC - Addon Debug Info Collector</em:name>
		<!--<em:code>adic</em:code> Still will be picked up by our script,
		                                   but won't bother the Mozilla guys.-->
		<em:version>1.0.7</em:version>
		<em:creator>Kevin Cox</em:creator>
		<em:contributor></em:contributor>
		<em:description>
			A tool that allows an average user to provide add-on developers
			useful information about their set-up to add-on developers.
		</em:description>

		<em:homepageURL>https://sourceforge.net/p/adic/wiki/Home/</em:homepageURL>

		<em:targetApplication>
			<Description>
				<em:id>{ec8030f7-c20a-464f-9b0e-13a3a9e97384}</em:id><!-- Firefox -->
				<em:minVersion>8.0</em:minVersion>
				<em:maxVersion>15.*</em:maxVersion>
			</Description>
		</em:targetApplication>

		<em:bootstrap>true</em:bootstrap>
		<em:iconURL>chrome://adic/content/logo.png</em:iconURL>

		<em:optionsURL>chrome://adic/content/options.xul</em:optionsURL>
		<em:optionsType>2</em:optionsType>
	</Description>
</RDF>
